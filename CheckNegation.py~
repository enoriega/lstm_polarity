input_path  ="SentencesInfo_all_label_final.csv"
with open(input_path) as f:
    reader = csv.DictReader(f)
    data = list(reader)
    
instances = [Instance.from_dict(d) for d in data]


for i in np.arange(1,len(instances),5000):
    instance = instances[i]
    print(instance.tokens[instance.start:instance.end])
    print(instance.neg_count)
